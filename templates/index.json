    "32c41e16-1668-4501-963b-0399f80ab824": {
      "type": "custom-about-text",
      "disabled": true,
      "settings": {
        "main_title": "About Crafty Halfling",
        "main_content": "Our brand focuses on clever ways to incorporate fantasy into everyday life. Crafty is a play on words of handmade crafts as well and cleverness.",
        "center_image": "shopify:\/\/shop_images\/about_center.png",
        "image_1": "shopify:\/\/shop_images\/Folder_4.png",
        "image_2": "shopify:\/\/shop_images\/Folder_7.png",
        "image_3": "shopify:\/\/shop_images\/Folder_5.png",
        "image_4": "shopify:\/\/shop_images\/Folder_1_1.png",
        "image_5": "shopify:\/\/shop_images\/Folder_3.png",
        "image_6": "shopify:\/\/shop_images\/Folder_2_1.png",
        "right_image": "shopify:\/\/shop_images\/about2.png",
        "content": "We make handmade goods such as monster dice bags, winged backpacks, dragon scarves, horns. We also build electronic props and costumes with a fantasy theme. Our customers are largely gamers, renaissance fair enthusiasts, and anime\/comic fans of all ages. We primarily sell at anime and comic book conventions, and online."
      }
    },
